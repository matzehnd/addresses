{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Deno (Allow All)",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": ["run", "--inspect", "-A", "main.ts"],
      "port": 9229
    },

    {
      "name": "Deno (Attach)",
      "type": "node",
      "request": "attach",
      "port": 9229
    }
  ]
}
